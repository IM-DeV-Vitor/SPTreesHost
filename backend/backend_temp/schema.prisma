datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}


model tree {
  id              String   @id @default(auto()) @map("_id") @db.ObjectId
  numero          Int
  vernaculo       String    
  cientify_name   String
  height          Float
  dap             Float
  phytosanitary_state String
  structure_state String
  suggested_management String
  coords Json
}
